# Laravel .gitignore for Railway deployment

# Dependencies
/vendor/
/node_modules/

# Environment files
.env
.env.local
.env.production
.env.railway

# Logs
/storage/logs/*
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/views/*

# Cache
/bootstrap/cache/*

# IDE files
.vscode/
.idea/
*.swp
*.swo

# OS files
.DS_Store
Thumbs.db

# Temporary files
*.tmp
*.temp

# Backup files
*.bak
*.backup

# Keep these files for Railway
!railway.json
!Procfile
!composer.json
!composer.lock